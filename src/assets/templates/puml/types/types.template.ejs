@startuml
skinparam componentStyle rectangle

<% if (graph.serviceTemplate[type]) {
const entries = Object.entries(graph.serviceTemplate[type])
for ([name, definition] of entries) { %>
rectangle "<%= name %>"
<% }} -%>

<% if (graph.serviceTemplate[type]) {
const entries = Object.entries(graph.serviceTemplate[type])
for ([name, definition] of entries) { %>
"<%= name %>" --|> "<%= definition.derived_from %>"
<% }} -%>

@enduml